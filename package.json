{
  "name": "mobx-ssr-kit",
  "version": "1.0.0",
  "description": "webpack4 + Mobx + ReactSSR",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --devtool eval --open --hot --compress --mode development --inline --profile --config config/webpack.dev.conf.js",
    "build": "npm run buildClean  &&  webpack  --mode production --progress --color --config config/webpack.prod.conf.js  &&  npm run pack",
    "analyz": "npm run buildClean  &&  webpack  --mode production --progress --color --profile --config config/webpack.prod.conf.js",
    "dll": "npm run dllClean  &&  webpack  --mode production --profile --config config/webpack.dll.conf.js -p",
    "buildClean": "rimraf ./dist",
    "dllClean": "rimraf ./dll",
    "pack": "node pack.js"
  },
  "keywords": [
    "mobx",
    "react-ssr",
    "webpack4"
  ],
  "author": "Edlan",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^9.0.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-import": "^1.8.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "cache-loader": "^1.2.2",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^1.0.0",
    "extended-define-webpack-plugin": "^0.1.3",
    "file-loader": "^1.1.11",
    "happypack": "^5.0.0",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.8.0",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.4.1",
    "postcss-loader": "^2.1.6",
    "prop-types": "^15.6.2",
    "react-hot-loader": "^4.3.3",
    "rimraf": "^2.6.2",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "url-loader": "^1.0.1",
    "webpack": "^4.16.1",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4",
    "webpack-manifest-plugin": "^2.0.3",
    "webpack-merge": "^4.1.3"
  },
  "dependencies": {
    "antd": "^3.7.0",
    "classnames": "^2.2.6",
    "koa": "^2.5.2",
    "koa-router": "^7.4.0",
    "mobx": "^5.0.3",
    "mobx-react": "^5.2.3",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-loadable": "^5.4.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1"
  }
}
