{
  "name": "mobx-admin-kit",
  "version": "1.0.0",
  "description": "webpack4 + Mobx + React16",
  "main": "index.js",
  "scripts": {
    "precommit": "npm run lint-staged",
    "dev": "cross-env NODE_ENV=dev webpack-dev-server --devtool eval --open --hot --compress --mode development --inline --profile --config config/webpack.dev.conf.js",
    "build": "cross-env webpack  --mode production --progress --color --config config/webpack.prod.conf.js",
    "analyzer": "cross-env ANALYZE=1 npm run build",
    "dll": "webpack  --mode production --profile --config config/webpack.dll.conf.js -p",
    "lint:style": "stylelint \"src/**/*.less\" --syntax less",
    "lint": "eslint --ext .js src && npm run lint:style",
    "lint:fix": "eslint --fix --ext .js src && npm run lint:style",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --fix --ext .js",
    "prettier": "prettier --write ./src/**/**/**/*"
  },
  "keywords": [
    "mobx",
    "react16",
    "webpack4",
    "admin"
  ],
  "author": "Edlan",
  "license": "ISC",
  "sideEffects": false,
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.2",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.1.5",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-import": "^1.9.1",
    "cache-loader": "^1.2.2",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "cssnano": "^4.1.0",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-babel": "^5.2.0",
    "eslint-plugin-compat": "^2.5.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-markdown": "^1.0.0-beta.6",
    "eslint-plugin-react": "^7.11.1",
    "extended-define-webpack-plugin": "^0.1.3",
    "file-loader": "^2.0.0",
    "happypack": "^5.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "image-webpack-loader": "^4.3.1",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "lint-staged": "^7.3.0",
    "mini-css-extract-plugin": "^0.4.3",
    "npm-run-all": "^4.1.3",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.14.3",
    "prop-types": "^15.6.2",
    "react-hot-loader": "^4.3.11",
    "style-loader": "^0.23.0",
    "stylelint": "^9.5.0",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-standard": "^18.2.0",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "url-loader": "^1.1.1",
    "webpack": "^4.19.1",
    "webpack-bundle-analyzer": "^3.0.2",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.8",
    "webpack-manifest-plugin": "^2.0.4",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "antd": "^3.12.3",
    "classnames": "^2.2.6",
    "mobx": "^4.5.2",
    "mobx-react": "^5.2.8",
    "moment": "^2.23.0",
    "path-to-regexp": "^2.4.0",
    "react": "^16.7.0",
    "react-dom": "^16.5.2",
    "react-loadable": "^5.5.0",
    "react-media": "^1.9.2",
    "react-router-dom": "^4.3.1"
  },
  "lint-staged": {
    "**/*.{js,less}": [
      "prettier --wirter",
      "git add"
    ],
    "**/*.{js}": "lint-staged:js",
    "**/*.less": "stylelint --syntax less"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie < 9"
  ]
}
